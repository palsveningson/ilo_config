<RIBCL VERSION="2.1">
<LOGIN USER_LOGIN="user" PASSWORD="user">
<RIB_INFO MODE="WRITE" >
<MOD_NETWORK_SETTINGS>
<DHCP_ENABLE VALUE = "Y"/>
<DNS_NAME VALUE = "<%=iloname%>"/>
<DHCP_GATEWAY VALUE = "Y"/>
<DHCP_DNS_SERVER VALUE = "Y"/>
