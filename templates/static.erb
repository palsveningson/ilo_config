<RIBCL VERSION="2.1">
<LOGIN USER_LOGIN="user" PASSWORD="user">
<RIB_INFO MODE="WRITE" >
<MOD_NETWORK_SETTINGS>
<DHCP_ENABLE VALUE = "N"/>
<DNS_NAME VALUE = "<%=iloname%>"/>
<IP_ADDRESS VALUE = "<%=staticip%>"/>
<SUBNET_MASK VALUE = "<%=netmask%>"/>
<GATEWAY_IP_ADDRESS VALUE = "<%=gateway%>"/>
<PRIM_DNS_SERVER value = "<%=pridns%>"/>
</MOD_NETWORK_SETTINGS>
</RIB_INFO>
</LOGIN>
</RIBCL>
